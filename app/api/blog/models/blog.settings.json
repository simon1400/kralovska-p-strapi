{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "2 Články -",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "add_title": {
      "type": "string"
    },
    "add_content": {
      "type": "richtext"
    },
    "cta": {
      "type": "component",
      "repeatable": false,
      "component": "links.button"
    }
  }
}
