      "required": false,
      "pluginOptions": {}
    },
    "articles": {
      "via": "category",
      "collection": "blog",
      "dominant": true
    },
    "meta": {
      "type": "component",
      "repeatable": false,
      "component": "seo.meta"
    }
  }
}
