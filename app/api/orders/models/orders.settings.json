{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "orders",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "surname": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    },
    "anotherAddress": {
      "type": "component",
      "repeatable": true,
      "component": "user.another-address"
    },
    "firmInfo": {
      "type": "component",
      "repeatable": true,
      "component": "user.firm-info"
    },
    "description": {
      "type": "text"
    },
    "basketItem": {
      "type": "component",
      "repeatable": true,
      "component": "products.basket-item"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "CREATED",
        "PAID",
        "CANCELED"
      ]
    },
    "sum": {
      "type": "biginteger"
    },
    "delivery": {
      "type": "component",
      "repeatable": false,
      "component": "orders.delivery-payment"
    },
    "payment": {
      "type": "component",
      "repeatable": false,
      "component": "orders.delivery-payment"
    },
    "payOnline": {
      "type": "boolean",
      "default": false
    },
    "sendMail": {
      "type": "boolean",
      "default": false
    }
  }
}
